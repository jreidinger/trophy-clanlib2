/**
 * ManStyle Themes Config for Theme "tec"
 * Author: Andreas Mustun
 */


/** 
 * User defined variables:
 */
[variables]
{
  /* Options */
  $opt_use_img_borders     = "yes"
  $opt_use_img_margins     = "no"
  $opt_screen_resolution   = "95"
  
  /* Colors */
  $color_bright            = "#dae6ec"
  $color_medium            = "#12568c"
  $color_dark              = "#023a64"
  
  /* Document settings */
  $doc_bg_image            = "img/tile.jpeg"
  $doc_bg_color            = "#ffffff"
  $doc_text_color          = "#000000"
  $doc_link_color          = "$color_medium"
  $doc_vlink_color         = "$color_dark"
  $doc_alink_color         = "#000000"
  $doc_navigator_color     = "#000000"
  
  $doc_total_width         = "615"
  $doc_left_border         = "30"
  $doc_margin_width        = "105"
  $doc_spacer_width        = "20"
  $doc_body_width          = "460"
  
  /* Separator between list entries: */
  $page_links_separator    = "&nbsp;|&nbsp;"
  $page_first              = "0"
  
  /* Page template */
  $page_template           = "page.html"
  $image_template          = "image.html"
  
  /* Headers */
  $header_format_1         = "level_1&nbsp;&nbsp;     title_anchor"
  $header_format_2         = "level_1.level_2&nbsp;   title_anchor"
  $header_format_3         = "level_1.level_2.level_3 title_anchor"
  $header_format_4         = "title_anchor"
  $header_format_5         = "title_anchor"
  $header_format_6         = "title_anchor"
  $header_format_7         = "title_anchor"
  $header_format_8         = "title_anchor"
  $header_format_9         = "title_anchor"
  
  /* Contents */
  $contents_depth          = "3"
  $contents_format_1       = "level_1&nbsp;&nbsp;     title_link"
  $contents_format_2       = "level_1.level_2&nbsp;   title_link"
  $contents_format_3       = "level_1.level_2.level_3 title_link">
  $contents_format_4       = "title_link">
  $contents_format_5       = "title_link">
  
  /* Misc */
  $date_format             = "DD/MM/YYYY"
}


/**
 * Styles
 */
[styles]
{
  <default>
  {
    font-family:"Arial","Helvetica","Geneva";
    font-size:10pt;
  }
  
  <pg>
  {
    text-indent:4mm;
    margin-left:0.5cm;
    margin-right:0.5cm;
    margin-top:0.2cm;
    margin-bottom:0.3cm;
  }
  
  <pgni>
  {
    margin-left:0.5cm;
    margin-right:0.5cm;
    margin-top:0.2cm;
    margin-bottom:0.3cm;
  }

  <overview>
  {
    font-family:"Times",serif;
    font-style:italic;
    font-size:12pt;
    margin-top:0.2cm;
    margin-bottom:0.6cm;
    margin-left:0.5cm;
    margin-right:0.5cm;
  }

  <code>
  {
    font-family:"Courier New","Courier";
    font-size:10pt;
    margin-top:0.2cm;
    margin-bottom:0.6cm;
    margin-left:0.5cm;
    margin-right:0.5cm;
    text-align:left;
  }
  
  <blockIndentLeft>
  {
    margin-left:0.5cm;
    margin-right:0cm;
