/**
 * NativeStyles
 */
[nativeStyles]
{
A:link {
  text-decoration:none;
}

OL,UL,BLOCKQUOTE {
  font-family:"Arial","Helvetica","Geneva";
  font-size:10pt;
  text-align:justify;
  margin-right:0.5cm;
  margin-top:0cm;
}

TD.navigator {
  background-color:$doc_navigator_color;
}

TD.tableHeader {
  background-color:$color_bright;
  font-family:"Arial","Helvetica","Geneva";
  font-size:10pt;
  font-weight:bold;
  text-align:center;
}
}


